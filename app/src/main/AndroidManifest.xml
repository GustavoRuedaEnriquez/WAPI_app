<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iteso.wapi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_wapi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_wapi_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivitySplashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityMain" />
        <activity android:name=".ActivityMateria" />
        <activity android:name=".ActivityLogin" />
        <activity android:name=".ActivityRegister" />
        <activity android:name=".ActivityEditHomework" />
        <activity android:name=".ActivityEditGrade" />
        <activity
            android:name=".ActivityHome"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ActivityEditSubject" />
        <activity android:name=".ActivityCreateSubject" />
        <activity android:name=".ActivityEditSchedule" />
        <activity android:name=".ActivityEditPeriod" />
        <activity android:name=".ActivityEditInformation" />

    </application>

</manifest>